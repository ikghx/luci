{
	"luci-app-frpc": {
		"description": "Grant UCI access for luci-app-frpc",
		"read": {
			"file": {
				"/etc/frp/frpc.ini": [ "read" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": [ "frpc" ]
		},
		"write": {
			"file": {
				"/etc/frp/frpc.ini": [ "write" ],
				"/etc/init.d/frpc restart": [ "exec" ]
			},
			"uci": [ "frpc" ]
		}
	}
}
