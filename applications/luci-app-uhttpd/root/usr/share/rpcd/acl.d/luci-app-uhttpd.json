{
	"luci-app-uhttpd": {
		"description": "Grant UCI access for luci-app-uhttpd",
		"read": {
			"uci": [ "uhttpd" ],
			"file": {
				"/etc/luci-uploads/*": ["read"]
			}
		},
		"write": {
			"uci": [ "uhttpd" ],
			"file": {
				"/etc/luci-uploads/*": ["write"],
				"/etc/init.d/uhttpd": ["exec"]
			}
		}
	}
}
