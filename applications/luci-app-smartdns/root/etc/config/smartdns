
config smartdns
	option server_name 'smartdns'
	option tcp_server '1'
	option ipv6_server '1'
	option dualstack_ip_selection '1'
	option prefetch_domain '0'
	option rr_ttl '300'
	option rr_ttl_min '60'
	option rr_ttl_max '600'
	option seconddns_tcp_server '0'
	option seconddns_no_speed_check '1'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_rule_soa '0'
	option seconddns_no_dualstack_selection '0'
	option seconddns_no_cache '0'
	option force_aaaa_soa '0'
	option coredump '0'
	option port '5336'
	option seconddns_port '5334'
	option seconddns_server_group 'public'
	option seconddns_enabled '1'
	option seconddns_no_dualstack_selection '1'
	option seconddns_no_cache '1'
	option redirect 'none'
	option cache_size '10000'
	option serve_expired '0'
	list old_redirect 'none'
	list old_port '5336'
	list old_enabled '0'

config server
	option enabled '1'
	option type 'udp'
	option ip '119.29.29.29'
	option port '53'
	option name 'DNSPod'
	option server_group 'china'
	option blacklist_ip '0'

