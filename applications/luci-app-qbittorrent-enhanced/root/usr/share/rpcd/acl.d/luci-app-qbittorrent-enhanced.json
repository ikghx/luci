{
	"luci-app-qbittorrent-enhanced": {
		"description": "Grant access to LuCI app qBittorrent Enhanced",
		"read": {
			"ubus": {
				"service": [ "list" ],
				"luci": [ "setInitAction" ]
			},
			"file": {
				"/etc/passwd": [ "read" ],
				"/usr/bin/qbittorrent-nox-enhanced": [ "exec" ]
			},
			"uci": [ "qbittorrent-enhanced" ]
		},
		"write": {
			"uci": [ "qbittorrent-enhanced" ]
		}
	}
}
