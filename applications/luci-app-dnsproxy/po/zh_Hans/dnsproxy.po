msgid "DNS Proxy"
msgstr "DNS 代理"

msgid "Simple DNS proxy with DoH, DoT, DoQ and DNSCrypt support."
msgstr "具有 DoH、DoT、DoQ 和 DNSCrypt 支持的简单 DNS 代理。"

msgid "Listen address"
msgstr "监听地址"

msgid "Enable log file"
msgstr "启用日志文件"

msgid "Log file path"
msgstr "日志文件路径"

msgid "Enable all servers"
msgstr "启用所有服务器"

msgid "Enable parallel queries to all configured upstream servers."
msgstr "对所有配置的上游服务器启用并行查询。"

msgid "Fastest address"
msgstr "最快的地址"

msgid "Respond to A or AAAA requests only with the fastest IP address."
msgstr "仅使用最快的 IP 地址响应 A 或 AAAA 请求。"

msgid "Disable TLS certificate validation"
msgstr "禁用 TLS 证书验证"

msgid "Disable IPv6"
msgstr "禁用 IPv6"

msgid "all AAAA requests will be replied with NoError RCode and empty answer."
msgstr "所有 AAAA 请求都将以 NoError RCode 和空答案回复。"

msgid "Max go routines"
msgstr "最大 go 协程"

msgid "Set the maximum number of go routines. A value <= 0 will not not set a maximum."
msgstr "设置 goroutines 的最大数量。值 <= 0 不会设置最大值。"

msgid "Ratelimit (requests per second)"
msgstr "速率限制（每秒请求数）"

msgid "refuse ANY requests"
msgstr "拒绝任何请求"

msgid "UDP buffer size (bytes)"
msgstr "UDP 缓冲区大小（字节）"

msgid "Verbose output"
msgstr "详细输出"

msgid "DNS Proxy - NXDOMAIN"
msgstr "DNS 代理 - NXDOMAIN"

msgid "Transform the responses containing at least a single IP that matches specified addresses and CIDRs into NXDOMAIN."
msgstr "将包含至少一个与指定地址和 CIDR 匹配的 IP 的响应转换为 NXDOMAIN"

msgid "DNS Proxy - Cache"
msgstr "DNS 代理 - 缓存"

msgid "Cache"
msgstr "缓存"

msgid "optimistic DNS cache is enabled"
msgstr "启用乐观 DNS 缓存"

msgid "Cache size (bytes)"
msgstr "缓存大小（字节）"

msgid "Minimum TTL value"
msgstr "最小 TTL 值"

msgid "Maximum TTL value"
msgstr "最大 TTL 值"

msgid "DNS Proxy - DNS64"
msgstr "DNS 代理 - DNS64"

msgid "DNS64 prefix"
msgstr "DNS64 前缀"

msgid "DNS Proxy - EDNS"
msgstr "DNS 代理 - EDNS"

msgid "Send EDNS Client Address"
msgstr "发送 EDNS 客户端地址"

msgid "DNS Proxy - Servers"
msgstr "DNS 代理 - 服务器"

msgid "Servers"
msgstr "服务器"

msgid "Bootstrap DNS"
msgstr "引导 DNS"

msgid "Fallback DNS"
msgstr "后备 DNS"

msgid "Upstream DNS"
msgstr "上游 DNS"
