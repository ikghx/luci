{
	"luci-app-pptpd": {
		"description": "Grant UCI access for luci-app-pptpd",
		"read": {
			"ubus": {
				"*": [ "*" ]
			},
			"uci": [ "pptpd", "firewall" ]
		},
		"write": {
			"ubus": {
				"*": [ "*" ]
			},
			"file": {
				"/etc/firewall.user": [ "write" ]
			},
			"uci": [ "pptpd", "firewall" ]
		}
	}
}
