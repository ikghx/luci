{
	"luci-app-vsftpd": {
		"description": "Grant UCI access for luci-app-vsftpd",
		"read": {
			"file": {
				"/var/log/vsftpd.log": [ "read" ]
			},
			"uci": [ "vsftpd" ]
		},
		"write": {
			"file": {
				"/var/log/vsftpd.log": [ "write" ],
				"/etc/init.d/vsftpd start" : [ "exec" ],
				"/etc/init.d/vsftpd stop" : [ "exec" ]
			},
			"uci": [ "vsftpd" ]
		}
	}
}
